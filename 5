using System;

class VerificaPlaca
{
    static void Main()
    {
        Console.Write("Digite a placa do ve√≠culo: ");
        string placa = Console.ReadLine();

        bool placaValida = placa.Length == 7;

        for (int i = 0; i < 3 && placaValida; i++)
        {
            if (!char.IsLetter(placa[i]))
            {
                placaValida = false;
            }
        }

        for (int i = 3; i < 7 && placaValida; i++)
        {
            if (!char.IsDigit(placa[i]))
            {
                placaValida = false;
            }
        }
        if (placaValida)
        {
            Console.WriteLine("Verdadeiro");
        }
        else
        {
            Console.WriteLine("Falso");
        }
    }
}
